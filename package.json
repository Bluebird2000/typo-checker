{
  "name": "typo-checker",
  "version": "1.0.4",
  "description": "Check for typos in variable names and string literals in a JavaScript/TypeScript codebase.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "source": "bin/index.ts",
  "bin": {
    "typo-checker": "dist/bin/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "devbuild": "tsup bin/index.ts --format esm,cjs --dts --out-dir dist",
    "build": "tsup lib/**/*.ts bin/**/*.ts --format esm,cjs --dts --out-dir dist",
    "prepublishOnly": "npm run build",
    "typo-checker": "node dist/index.js"
  },
  "typoChecker": {
    "whitelist": [
      "exampleVariable",
      "anotherExample",
      "myFunction",
      "someConstant"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bluebird2000/typo-checker.git"
  },
  "homepage": "https://github.com/Bluebird2000/typo-checker#readme",
  "keywords": [
    "typo",
    "typo-checker",
    "spellcheck",
    "spellchecker",
    "typo detection",
    "misspelling",
    "variable typos",
    "string typo",
    "string spellcheck",
    "variable name linter",
    "code quality",
    "static analysis",
    "code scanner",
    "typescript linter",
    "javascript linter",
    "code hygiene",
    "naming convention",
    "codebase audit",
    "lint tool",
    "developer tool"
  ],
  "author": "https://github.com/Bluebird2000/typo-checker",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^8.33.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "dictionary-en": "^4.0.0",
    "fast-glob": "^3.2.12",
    "nspell": "^2.1.5"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/nspell": "^2.1.6",
    "tsup": "^8.4.0",
    "typescript": "^4.0.0"
  }
}
