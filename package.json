{
  "name": "typo-checker",
  "version": "1.0.0",
  "description": "Check for typos in variable names and string literals in a JavaScript/TypeScript codebase.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "source": "bin/index.ts",
  "bin": {
    "typo-checker": "dist/index.cjs.js"
  },
  "files": [
    "dist",
    "dictionary.txt"
  ],
  "scripts": {
  "build": "tsup bin/index.ts --format esm,cjs --dts --out-dir dist",
  "prepublishOnly": "npm run build",
  "typo-checker": "node dist/index.js"
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bluebird2000/@bluebird-helpers.git"
  },
  "keywords": [
    "typo",
    "checker",
    "spellcheck",
    "lint",
    "variable"
  ],
  "author": "Bluebird - https://github.com/Bluebird2000",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.0.0",
    "chalk": "^5.3.0",
    "fast-glob": "^3.2.12",
    "leven": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.4",
    "tsup": "^8.4.0",
    "typescript": "^4.0.0"
  }
}
